            &lt;tube-factory
                    className="com.sun.xml.ws.assembler.jaxws.TerminalTubeFactory"/&gt;
            &lt;tube-factory
                    className="com.sun.xml.ws.assembler.jaxws.HandlerTubeFactory"/&gt;
            &lt;tube-factory
